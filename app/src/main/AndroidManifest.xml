<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hougr.com.hougrleakcanary" >

    <application
        android:name="com.hougr.leakcanary.ExampleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.hougr.leakcanary.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hougr.leakcanary.SingletonActivity" />
        <activity android:name="com.hougr.leakcanary.HandlerNonstaticActivity" />
        <activity android:name="com.hougr.leakcanary.AsyncTaskLeakActivity"></activity>
    </application>

</manifest>